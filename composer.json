{
    "name": "tobymaxham/ux9-api",
    "description": "The offical UX9 URL Shortener API.",
    "keywords": ["tobymaxham", "url", "shortener", "ux9"],
    "license": "MIT",
    "authors": [
        {
            "name": ":TobyMaxham",
            "email": "git@maxham.de",
            "homepage": "https://maxham.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://ux9.de",
    "support": {
        "issues": "https://github.com/TobyMaxham/ux9-api/issues",
        "source": "https://github.com/TobyMaxham/ux9-api"
    },
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "TobyMaxham\\Ux9\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "./vendor/bin/pint src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
